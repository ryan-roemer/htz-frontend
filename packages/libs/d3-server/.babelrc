{
  "presets": ["@haaretz/htz-react-base/babel.js"],
  "plugins": ["babel-plugin-dynamic-import-node"],
  "env": {
    "commonjs": {
      "ignore": ["**/*{.,-}{spec,test}.{js,jsx}", "**/{__tests__,__mocks__,__snapshots__}"]
    },
    "esnext": {
      "ignore": ["**/*{.,-}{spec,test}.{js,jsx}", "**/{__tests__,__mocks__,__snapshots__}"]
    },
    "esm": {
      "ignore": ["**/*{.,-}{spec,test}.{js,jsx}", "**/{__tests__,__mocks__,__snapshots__}"]
    }
  }
}
