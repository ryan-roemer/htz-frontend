{
  "name": "@haaretz/fela-utils",
  "version": "0.1.0",
  "description": "General, non-ui helpers for building Fela apps and components in the Haaretz stack",
  "keywords": [],
  "author": "TxHawks",
  "license": "UNLICENSED",
  "repository": "Haaretz/htz-frontend",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/esnext/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "htz-scripts build",
    "build:esm": "htz-scripts build:esm",
    "watch": "htz-scripts build:esm watch",
    "build:all": "yarn run build && yarn run build:docs",
    "build:docs": "rimraf docs/**/* && documentation build src --config documentation.yml -f html -o docs",
    "clean": "htz-scripts clean",
    "format": "htz-scripts format",
    "gc": "htz-scripts commitizen",
    "lint": "htz-scripts lint",
    "prepare": "npm run build",
    "serve": "yarn documentation serve src --config documentation.yml --watch",
    "styleguide": "htz-scripts styleguide",
    "styleguide:build": "htz-scripts styleguide:build",
    "test": "htz-scripts test",
    "update-scripts": "htz-scripts update-scripts"
  },
  "dependencies": {
    "bidi-css-js": "^3.1.0",
    "enzyme": "^3.7.0",
    "enzyme-to-json": "^3.3.4",
    "fela-beautifier": "^5.0.15",
    "fela-bindings": "^2.2.0",
    "fela-combine-arrays": "^1.0.7",
    "fela-perf": "^5.0.3",
    "fela-plugin-embedded": "^5.2.7",
    "fela-plugin-extend": "^6.0.11",
    "fela-plugin-fallback-value": "^5.0.17",
    "fela-plugin-lvha": "^5.0.15",
    "fela-plugin-placeholder-prefixer": "^5.0.18",
    "fela-plugin-unit": "^5.0.16",
    "fela-plugin-validator": "^5.1.1",
    "fela-statistics": "^5.0.20",
    "fela-tools": "^5.1.5",
    "inline-style-prefixer": "^5.0.3",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react-test-renderer": "^16.8.2"
  },
  "devDependencies": {
    "@haaretz/htz-react-base": "^1.1.0",
    "babel-loader": "^8.0.0-beta.4",
    "documentation": "^8.1.2",
    "fela": "^6.1.7",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-fela": "^7.2.0",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "fela": "^6.1.7",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-fela": "^7.2.0"
  }
}
