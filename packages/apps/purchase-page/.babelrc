{
  "presets": ["next/babel"],
  "env": {
    "development": {
      "plugins": [
        [
          "lodash",
          {
            "id": [
              "lodash",
              "@haaretz/htz-components",
              "@haaretz/htz-theme",
              "@haaretz/tm-theme",
              "@haaretz/fela-utils",
              "@haaretz/app-utils"
            ]
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "lodash",
          {
            "id": [
              "lodash",
              "@haaretz/htz-components",
              "@haaretz/htz-theme",
              "@haaretz/tm-theme",
              "@haaretz/fela-utils",
              "@haaretz/app-utils"
            ]
          }
        ]
      ]
    },
    "commonjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs"
          }
        ]
      ]
    }
  }
}
