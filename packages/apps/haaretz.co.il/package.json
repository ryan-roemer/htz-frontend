    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "concurrently": "^4.0.1",
    "config": "^1.26.2",
    "cross-env": "^5.2.0",
    "dataloader": "^1.3.0",
    "fela": "^6.1.7",
    "graphql": "^14.0.2",
    "graphql-anywhere": "^4.1.11",
    "graphql-tag": "^2.9.2",
    "isomorphic-unfetch": "^2.1.1",
    "lodash": "^4.17.10",
    "next": "^8.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.8.2",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.2",
    "react-fela": "^7.2.0",
    "react-ga": "^2.5.3",
    "react-intersection-observer": "^3.0.2",
    "universal-cookie": "^2.0.8"
  },
  "devDependencies": {
