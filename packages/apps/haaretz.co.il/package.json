    "@haaretz/htz-react-base": "^1.1.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-lodash": "^3.3.4",
    "enzyme-to-json": "^3.3.4",
    "glob": "^7.1.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4"
  }
}
