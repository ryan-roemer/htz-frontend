  "license": "UNLICENSED",
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both htz-scripts build",
    "analyze:browser": "BUNDLE_ANALYZE=browser htz-scripts build",
    "analyze:server": "BUNDLE_ANALYZE=server htz-scripts build",
    "build": "yarn clean && htz-scripts build",
    "build:deploy": "lerna run --stream --scope=@haaretz/htz-user-utils --scope=@haaretz/app-utils --scope=@haaretz/htz-react-server --scope=@haaretz/graphql-server --scope=@haaretz/haaretz.co.il prepare",
    "clean": "htz-scripts clean",
    "debug": "htz-react-server htz --inspect",
    "dev": "cross-env concurrently \"cross-env NODE_ENV=development graphql-server\" \" cross-env NODE_ENV=development htz-react-server htz | bunyan -L\"",
    "dev:2prod": "cross-env concurrently \"cross-env NODE_ENV=production graphql-server\" \"cross-env NODE_ENV=development CONNECTION_PRESET=dev2prod htz-react-server htz | bunyan -L\"",
    "dev:debug": "cross-env concurrently \"cross-env NODE_ENV=development graphql-server\" \" cross-env NODE_ENV=development node --inspect-brk ./node_modules/.bin/htz-react-server htz | bunyan -L\"",
    "dev:dev": "cross-env concurrently \"cross-env NODE_ENV=development graphql-server\" \"cross-env CONNECTION_PRESET=dev NODE_ENV=development htz-react-server htz | bunyan -L\"",
    "format": "htz-scripts format",
    "gc": "htz-scripts commitizen",
    "lint": "htz-scripts lint",
    "postinstall": "htz-scripts fixStyledJsx",
    "prepare": "npm run build",
    "start:dev2prod": "cross-env concurrently \"cross-env NODE_ENV=production graphql-server\" \"cross-env CONNECTION_PRESET=dev2prod NODE_ENV=production htz-react-server htz | bunyan -L\"",
    "start:dev2stage": "cross-env concurrently \"cross-env NODE_ENV=development graphql-server\" \"cross-env CONNECTION_PRESET=dev2prod NODE_ENV=production htz-react-server htz | bunyan -L\"",
    "start:gql": "cross-env  NODE_ENV=production graphql-server",
    "start:gql2dev": "cross-env  NODE_ENV=development graphql-server",
    "start:next": "cross-env  NODE_ENV=production htz-react-server htz",
    "test": "htz-scripts test",
    "update-scripts": "htz-scripts update-scripts"
  },
  "dependencies": {
    "@haaretz/app-utils": "^0.1.0",
