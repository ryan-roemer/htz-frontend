{
  "env": {
    "development": {
      "plugins": [[ "lodash", { "id": [ "lodash", "@haaretz/htz-components" ] } ]],
      "presets": "next/babel"
    },
    "production": {
      "plugins": [
        [
          "lodash",
          {
            "id": [
              "lodash",
              "@haaretz/htz-components",
              "@haaretz/htz-theme",
              "@haaretz/tm-theme",
              "@haaretz/fela-utils",
              "@haaretz/app-utils"
            ]
          }
        ]
      ],
      "presets": "next/babel"
    },
    "commonjs": {
      "presets": [["env", { "modules": "commonjs" }], "next/babel"]
    }
  }
}
